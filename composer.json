{
"repositories": [
   {
     "type": "vcs",
     "url": "https://github.com/philipbrown/commonmark"
   }
  ],
	"require": {
		"laravel/framework": "~4.2",
		"mitch/laravel-doctrine": "~0.5",
		"beberlei/assert": "~2.0",
		"rhumsaa/uuid": "~2.7",
		"league/commonmark": "dev-remove-deprecated-interface"
	},
	"require-dev": {
		"phpunit/phpunit": "4.0",
		"mockery/mockery": "~0.8",
		"doctrine/data-fixtures": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Cribbb\\": "src/"
		},
		"classmap": [
			"app/controllers",
			"app/database/migrations",
			"app/database/seeds",
			"tests/TestCase.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Cribbb\\Tests\\": "tests/",
			"Cribbb\\Stubs\\": "stubs/"
		}
	},
	"scripts": {
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
